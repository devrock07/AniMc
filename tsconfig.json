{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
		"importsNotUsedAsValues": "remove",
		"preserveValueImports": false,
		"ignoreDeprecations": "5.0",
		"paths": {
			"$lib": [
				"src/lib"
			],
			"$lib/*": [
				"src/lib/*"
			],
			"@components": [
				"src/lib/components"
			],
			"@components/*": [
				"src/lib/components/*"
			],
			"@scripts": [
				"src/lib/scripts"
			],
			"@scripts/*": [
				"src/lib/scripts/*"
			]
		}
	}
}